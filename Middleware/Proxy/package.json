  "version": "0.0.1",
  "description": "survey-backend-middleware-proxy",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --no-notify src/app.ts",
    "start": "npm run clean && npm run build && node dist/app.js",
    "build": "npm run clean && tsc -b",
    "clean": "tsc -b --clean",
    "debug": "node --inspect -r ts-node/register src/app.ts",
    "docker-build": "npm run docker-stop && docker-compose build && docker rmi -f $(docker images --quiet --filter 'dangling=true')",
    "docker-start": "docker-compose up",
    "docker-stop": "docker-compose down",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
